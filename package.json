{
  "name": "markvista-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "export": "NODE_ENV=production next build",
    "export:verify": "npm run export && node scripts/verify-build.js",
    "deploy": "npm run export:verify && touch out/.nojekyll",
    "export:docs": "npm run export && rm -rf docs && mkdir -p docs && cp -R out/* docs/ && touch docs/.nojekyll",
    "deploy:docs": "npm run export:docs && echo 'Docs prepared. Push and set Pages source to main/docs.'"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "docx": "^9.5.1",
    "file-saver": "^2.0.5",
    "html2pdf.js": "^0.12.1",
    "katex": "^0.16.25",
    "lucide-react": "^0.552.0",
    "mermaid": "^11.12.1",
    "next": "16.0.1",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-markdown": "^10.1.0",
    "react-swipeable": "^7.0.2",
    "react-syntax-highlighter": "^16.1.0",
    "react-window": "^2.2.2",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/file-saver": "^2.0.7",
    "@types/html2canvas": "^0.5.35",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/react-window": "^1.8.8",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}